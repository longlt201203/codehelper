{
  "name": "codehelper",
  "displayName": "codehelper",
  "description": "",
  "version": "0.0.3",
  "publisher": "longlt201203",
  "engines": {
    "vscode": "^1.89.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "codehelper.setUp",
        "title": "CodeHelper: Set Up"
      },
      {
        "command": "codehelper.document",
        "title": "CodeHelper: Document"
      },
      {
        "command": "codehelper.askGemini",
        "title": "CodeHelper: Ask Gemini"
      }
    ],
    "configuration": {
      "title": "CodeHelper",
      "properties": {
        "codehelper.googleApiKey": {
          "type": "string",
          "description": "Google API Key for calling Gemini API"
        },
        "codehelper.language": {
          "type": "string",
          "description": "Language for generating code"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.89.0",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.3.9",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@google/generative-ai": "^0.11.3"
  },
  "repository": "https://github.com/longlt201203/codehelper.git",
  "author": "Le Thanh Long <longlt201203@gmail.com>",
  "license": "MIT"
}